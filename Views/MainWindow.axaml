                    VerticalAlignment="Center"
                    Margin="0" />
            <Button x:Name="Supplies"
                    Content="Оборудование"
                    Grid.Column="1"
                    Grid.Row="0"
                    Width="200"
                    Height="30"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Margin="0" />
            <Button x:Name="Employee"
                    Content="Сотрудники"
